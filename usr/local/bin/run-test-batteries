#!/bin/bash

cd ~
export TMPDIR=${TMPDIR:-/var/tmp}

FCM_VERSION=${1:-} fcm test-battery 2>&1 | tee test-fcm.out.$(date +%Y-%m-%dT%H%M)
CYLC_VERSION=${1:-} cylc test-battery 2>&1 | tee test-cylc.out.$(date +%Y-%m-%dT%H%M)
ROSE_VERSION=${1:-} rose test-battery 2>&1 | tee test-rose.out.$(date +%Y-%m-%dT%H%M)
